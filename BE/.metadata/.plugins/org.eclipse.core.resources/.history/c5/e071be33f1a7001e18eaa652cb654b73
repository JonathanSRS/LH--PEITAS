package br.com.lhp.model;

import java.sql.Date;

public class camisa {
	private static Date temporada;
	private static int cod;
	private static char status;
	private static String cor;
	private static String nome;
	private static String informativo;
	
	public camisa(int cod, String nome, Date temporada, String cor) {
		camisa.cod = cod;
		camisa.nome = nome;
		camisa.temporada = temporada;
		camisa.cor = cor;
		camisa.status = 'A';
	}

	public static String getNome() {
		return nome;
	}

	public static Date getTemporada() {
		return temporada;
	}

	public static String getCor() {
		return cor;}

	public static String getInformativo() {
		return informativo;
	}

	public static char getStatus() {
		return status;
	}
	
	public static int getCod() {
		return cod;
	}

	public static void setNome(String nome) {
		camisa.nome = nome;
	}
	
	public static void setInformativo(String informativo) {
		camisa.informativo = informativo;
	}
	
	public static void setStatus(char status) {
		camisa.status = status;
	}

}
