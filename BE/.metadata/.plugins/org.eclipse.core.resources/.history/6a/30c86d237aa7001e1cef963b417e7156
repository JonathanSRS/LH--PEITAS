package br.com.lhp.model;

public class imagem {
	private static String pasta;
	private static String conteudo;
	public static String getPasta() {
		return pasta;
	}
	public static void setPasta(String pasta) {
		imagem.pasta = pasta;
	}
	public static String getConteudo() {
		return conteudo;
	}
	public static void setConteudo(String conteudo) {
		imagem.conteudo = conteudo;
	}
	
	public imagem(String pasta, String conteudo) {
		imagem.pasta = pasta;
		imagem.conteudo = conteudo;
	}

}
