package br.com.lhp.model;

import java.sql.Date;

public class Uniforme {
	private static String temporada;
	private static int cod;
	private static char status;
	private static String cor;
	private static String nome;
	private static String informativo;
	
	public Uniforme(String nome, String temporada, String cor) {
		Uniforme.nome = nome;
		Uniforme.temporada = temporada;
		Uniforme.cor = cor;
		Uniforme.status = 'A';
	}

	public String getNome() {
		return nome;
	}

	public String getTemporada() {
		return temporada;
	}

	public String getCor() {
		return cor;}

	public String getInformativo() {
		return informativo;
	}

	public char getStatus() {
		return status;
	}
	
	public int getCod() {
		return cod;
	}

	public void setNome(String nome) {
		Uniforme.nome = nome;
	}
	
	public void setInformativo(String informativo) {
		Uniforme.informativo = informativo;
	}
	
	public void setStatus(char status) {
		Uniforme.status = status;
	}

}
